{
  "name": "cache-storage",
  "description": "It is just like localStorage - but built on top of Cache API",
  "version": "0.0.0-semantic-release",
  "main": "src/cache-storage.js",
  "dependencies": {},
  "devDependencies": {
    "pre-git": "3.1.1",
    "standard": "5.4.1",
    "webpack": "1.12.9",
    "semantic-release": "^4.3.5"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "standard src/*.js",
    "build": "webpack",
    "commit": "commit-wizard",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/caches-storage.git"
  },
  "keywords": [
    "storage",
    "api",
    "cache",
    "caches",
    "worker"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/caches-storage/issues"
  },
  "homepage": "https://github.com/bahmutov/caches-storage#readme",
  "config": {
    "pre-git": {
      "commit-msg": [
        "simple"
      ],
      "pre-commit": [
        "npm run test"
      ],
      "pre-push": [
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  }
}
